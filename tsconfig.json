{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@components/*": ["src/components/*"],
			"@lib/*": ["src/lib/*"],
			"@layouts/*": ["src/layouts/*"],
			"@src/*": ["src/*"],
			"@assets/*": ["src/assets/*"]
		},
		"types": ["unplugin-icons/types/astro"],
		"verbatimModuleSyntax": true
	}
}
